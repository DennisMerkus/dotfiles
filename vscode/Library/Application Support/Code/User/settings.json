{
  "telemetry.enableTelemetry": false,

  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Ayu Mirage",
  "workbench.iconTheme": "ayu",

  "editor.fontSize": 15,
  "editor.fontFamily": "'Fira Code', 'Consolas', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "solid",
  "editor.renderLineHighlight": "all",

  "terminal.external.osxExec": "Hyper.app",
  "terminal.integrated.shell.osx": "/bin/zsh",

  "vs-kubernetes": {
    "vs-kubernetes.helm-path.mac": "/Users/dennis/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.draft-path.mac": "/Users/dennis/.vs-kubernetes/tools/draft/darwin-amd64/draft",
    "vs-kubernetes.minikube-path.mac": "/Users/dennis/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },

  "files.exclude": {
    "**/__pycache__": true
  },

  "python.jediEnabled": true,

  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.formatting.provider": "black",

  "editor.tabCompletion": "on",

  "editor.formatOnSave": false,

  "editor.codeActionsOnSave": {
    "source.organizeImports": false,
    "source.fixAll.stylelint": false,
  },

  "eslint.alwaysShowStatus": true,

  "typescript.format.enable": false,

  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp",
    "editor.formatOnSave": true,
  },
  "[css]": {
    "editor.suggest.insertMode": "replace",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    }
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
  },
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true ,
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    }
  },
  "[rust]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rust-lang.rust",
  },

  "tailwindCSS.emmetCompletions": true,

  "window.zoomLevel": 0,
  "editor.renderControlCharacters": false,

  "omnisharp.enableRoslynAnalyzers": true,
"omnisharp.defaultLaunchSolution": "null",
"python.languageServer": "Jedi",
"[jsonc]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"javascript.updateImportsOnFileMove.enabled": "always",
}
